{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "scripts": {
    "start": "webpack-serve --port 8080",
    "start-dev": "webpack-dev-server --mode development --watch-content-base --open",
    "start-full": "npm-run-all -p start-mqtt-server start-node-backend start",
    "start-mqtt-server": "node ../API/mqttserver.js",
    "start-node-backend": "node ../API/index.js",
    "test": "karma start",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "migrate": "webpack-cli migrate ./config/webpack.common.js",
    "dev": "webpack --mode development"
  },
  "license": "MIT",
