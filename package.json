    "@types/jszip": "^3.1.4",
    "@types/node": "^11.9.4",
    "@types/redux-logger": "^3.0.0",
    "@types/uuid": "^3.4.4",
    "@types/yargs": "12.0.9",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "@vizuaalog/bulmajs": "^0.10.2",
    "bpmnlint": "^6.2.0",
    "codelyzer": "~4.5.0",
    "cypress": "^3.8.3",
